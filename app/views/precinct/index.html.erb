<% if @locality %>
<%= link_to(@locality.source.name, @locality.source) %> &gt;&gt; 
<%= @locality.state.name %> &gt;&gt; 
<%= link_to(@locality.name, @locality) %> &gt;&gt; Precincts
<% elsif @source %>
<%= link_to(@source.name, @source) %>
<% end %>
<table>
<tr><th>locality</th><th>name</th></tr>
<% @precincts.each do |p| -%>
<tr>
<td><%= link_to p.locality.name, p.locality %></td>
<td><%= link_to p.name, :controller => "precinct", :action => "show", :id => p.id %></td></tr>
<% end -%>
</table>
